    <profile name="Cyclic Ion Blaster" typeId="8e6f-7553-28c5-95a0" typeName="Weapons" hidden="false" id="026c-b961-ed6d-f146">
      <characteristics>
        <characteristic name="Range" typeId="9b9e-0c24-e409-a801">18&quot;</characteristic>
