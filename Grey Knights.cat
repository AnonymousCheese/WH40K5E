    <profile name="Nemesis Daemon Hammer" typeId="1d88-1369-f0ed-b39c" typeName="Melee Weapons" hidden="false" id="74e2-b19f-7313-eb5e">
      <characteristics>
        <characteristic name="Ability" typeId="2113-98a2-4376-a94c">This is a Nemesis Force Weapon which follows the rules for a Thunder Hammer. Note that a Nemesis Dreadknight armed with this weapon strikes at normal Initiative rather than Initiative 1.</characteristic>
      </characteristics>
    </profile>
    <profile name="Nemesis Warding Stave" typeId="1d88-1369-f0ed-b39c" typeName="Melee Weapons" hidden="false" id="1deb-a37b-fa22-ff45">
      <characteristics>
        <characteristic name="Ability" typeId="2113-98a2-4376-a94c">This is a Nemesis Force Weapon which grants the wielder a 2+ Invulnerable save against wounds caused in close combat.</characteristic>
      </characteristics>
    </profile>
    <profile name="Nemesis Falchions" typeId="1d88-1369-f0ed-b39c" typeName="Melee Weapons" hidden="false" id="2db1-5616-6e7e-0efa">
      <characteristics>
        <characteristic name="Ability" typeId="2113-98a2-4376-a94c">This is a pair of Nemesis Force Weapons which grants the wielder +1 Attack.</characteristic>
      </characteristics>
    </profile>
    <profile name="Nemesis Doomfist" typeId="1d88-1369-f0ed-b39c" typeName="Melee Weapons" hidden="false" id="f72f-d5fa-e8b7-5302">
      <characteristics>
