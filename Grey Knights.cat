    <profile name="Nemesis Force Sword" typeId="1d88-1369-f0ed-b39c" typeName="Melee Weapons" hidden="false" id="64f0-ab46-acac-8862">
      <characteristics>
        <characteristic name="Ability" typeId="2113-98a2-4376-a94c">This is a Nemesis Force Weapon which increases the bearer&apos;s Invulnerable save by +1 against close combat attacks. If the bearer has no Invulnerable save, there is no effect.</characteristic>
      </characteristics>
    </profile>
    <profile name="Nemesis Force Halberd" typeId="1d88-1369-f0ed-b39c" typeName="Melee Weapons" hidden="false" id="d9dc-c6c6-7f2e-1642">
      <characteristics>
